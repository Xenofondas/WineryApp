                  x:Class="WineryApp.Views.RootPage"
                  xmlns:Local="clr-namespace:WineryApp.Views;assebmly=WineryApp"
                  MasterBehavior="Popover">                


    <MasterDetailPage.Master>        
                <Local:MasterPage x:Name="masterPage" />           
    </MasterDetailPage.Master>

    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <Local:StartPage/>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
    
    
</MasterDetailPage>